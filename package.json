{
	"name": "generator-nsis",
	"version": "0.10.2",
	"description": "Yeoman generator for NSIS scripts",
	"author": "Jan T. Sott",
	"license": "MIT",
	"keywords": [
		"yeoman-generator",
		"nsis",
		"nullsoft",
		"installer",
		"setup"
	],
	"type": "module",
	"exports": "./generators/app",
	"files": [
		"generators",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"lint:ejs": "ejslint generators/**/*.ejs",
		"lint:js": "eslint generators/**/*.js --ignore-path .gitignore",
		"lint:json": "eslint ./*.json --ignore-path .gitignore",
		"lint": "npm-run-all --parallel lint:*",
		"prepack": "rimraf installer.nsi license.txt",
		"prepare": "husky",
		"publish": "np --no-yarn",
		"test": "uvu --ignore test/__helper.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/generator-nsis"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"@nsis/language-data": "^0.9.2",
		"@sindresorhus/slugify": "^2.2.1",
		"glob": "^10.3.10",
		"makensis": "^2.0.8",
		"semver": "^7.5.4",
		"spdx-license-list": "^6.8.0",
		"terminal-link": "^3.0.0",
		"yeoman-generator": "^7.1.1"
	},
	"devDependencies": {
		"@lukeed/uuid": "^2.0.1",
		"ejs-lint": "^2.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-json": "^3.1.0",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.1",
		"mem-fs": "^4.0.0",
		"np": "^9.2.0",
		"npm-run-all2": "^6.1.2",
		"prettier": "^3.2.5",
		"rimraf": "^5.0.5",
		"tsm": "^2.3.0",
		"uvu": "^0.5.6",
		"yeoman-assert": "^3.1.1",
		"yeoman-environment": "^4.3.0",
		"yeoman-test": "^8.2.0"
	},
	"lint-staged": {
		"*.ejs": "ejslint",
		"*.(js|json)": [
			"eslint --cache --fix",
			"prettier --write"
		]
	},
	"packageManager": "pnpm@8.15.2+sha256.90bb5d6382cb2cb8b8d4959a076b3953d84d1d94121717097bcd41c71344fa14"
}

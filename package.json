{
	"name": "generator-nsis",
	"version": "0.11.0",
	"description": "Yeoman generator for NSIS scripts",
	"author": "Jan T. Sott",
	"license": "MIT",
	"keywords": [
		"yeoman-generator",
		"nsis",
		"nullsoft",
		"installer",
		"setup"
	],
	"type": "module",
	"exports": "./generators/app",
	"files": [
		"generators",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"lint:ejs": "ejslint generators/**/*.ejs",
		"lint:code": "eslint **/*.json ./generators/**/*.js --no-warn-ignored",
		"lint": "concurrently --prefix '{name}' -c 'green,blue' 'npm:lint:*'",
		"prepack": "rimraf installer.nsi license.txt",
		"prepare": "husky",
		"publish:npm": "np --no-yarn --any-branch",
		"test": "uvu --ignore tests/__helper.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/generator-nsis"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"@nsis/language-data": "^0.9.2",
		"@sindresorhus/slugify": "^2.2.1",
		"glob": "^10.4.5",
		"makensis": "3.0.0-alpha.3",
		"semver": "^7.6.3",
		"spdx-license-list": "^6.9.0",
		"terminal-link": "^3.0.0",
		"yeoman-generator": "^7.3.3"
	},
	"devDependencies": {
		"@lukeed/uuid": "^2.0.1",
		"@types/node": "^22.9.0",
		"concurrently": "^9.1.0",
		"ejs-lint": "^2.0.1",
		"eslint": "9.13.0",
		"eslint-plugin-jsonc": "^2.18.1",
		"eslint-plugin-unicorn": "^56.0.0",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"mem-fs": "^4.1.1",
		"np": "^10.0.7",
		"prettier": "^3.3.3",
		"rimraf": "^5.0.10",
		"tsm": "^2.3.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "8.12.2",
		"uvu": "^0.5.6",
		"yeoman-assert": "^3.1.1",
		"yeoman-environment": "^4.4.3",
		"yeoman-test": "^8.3.0"
	},
	"lint-staged": {
		"*.ejs": "ejslint",
		"*.(js|json)": [
			"eslint --cache --fix",
			"prettier --write"
		]
	},
	"packageManager": "pnpm@8.15.9+sha512.499434c9d8fdd1a2794ebf4552b3b25c0a633abcee5bb15e7b5de90f32f47b513aca98cd5cfd001c31f0db454bc3804edccd578501e4ca293a6816166bbd9f81"
}

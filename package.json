{
	"name": "generator-nsis",
	"version": "0.11.2",
	"description": "Yeoman generator for NSIS scripts",
	"author": "Jan T. Sott",
	"license": "MIT",
	"keywords": [
		"yeoman-generator",
		"nsis",
		"nullsoft",
		"installer",
		"setup"
	],
	"type": "module",
	"exports": "./generators/app",
	"files": [
		"generators",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"lint:ejs": "ejslint generators/**/*.ejs",
		"lint:code": "biome check",
		"lint": "concurrently --prefix '{name}' -c 'green,blue' 'npm:lint:*'",
		"prepack": "rimraf installer.nsi license.txt",
		"prepare": "lefthook install",
		"publish:npm": "np --any-branch",
		"test": "uvu --ignore tests/__helper.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/idleberg/generator-nsis.git"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"@nsis/language-data": "^0.9.3",
		"@sindresorhus/slugify": "^2.2.1",
		"glob": "^11.0.3",
		"makensis": "3.0.0",
		"semver": "^7.7.2",
		"spdx-license-list": "^6.10.0",
		"terminal-link": "^4.0.0",
		"yeoman-generator": "^7.5.1"
	},
	"devDependencies": {
		"@idleberg/configs": "^0.2.0",
		"@lukeed/uuid": "^2.0.1",
		"@types/node": "^22.15.33",
		"concurrently": "^9.2.0",
		"ejs-lint": "^2.0.1",
		"mem-fs": "^4.1.2",
		"np": "^10.2.0",
		"prettier": "^3.6.1",
		"rimraf": "^6.0.1",
		"tsm": "^2.3.0",
		"typescript": "^5.8.3",
		"uvu": "^0.5.6",
		"yeoman-assert": "^3.1.1",
		"yeoman-environment": "^4.4.3",
		"yeoman-test": "^10.1.1"
	},
	"lint-staged": {
		"*.ejs": "ejslint",
		"*.(js|json)": [
			"prettier --write"
		]
	},
	"packageManager": "pnpm@10.12.3+sha512.467df2c586056165580ad6dfb54ceaad94c5a30f80893ebdec5a44c5aa73c205ae4a5bb9d5ed6bb84ea7c249ece786642bbb49d06a307df218d03da41c317417"
}

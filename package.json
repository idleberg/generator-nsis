{
  "name": "generator-nsis",
  "version": "0.8.5",
  "description": "Yeoman generator for NSIS scripts",
  "main": "generators/app",
  "files": [
    "generators"
  ],
  "scripts": {
    "lint:ejs": "ejslint generators/**/*.ejs",
    "lint:js": "eslint generators/**/*.js",
    "lint:json": "jsonlint --quiet ./*.json",
    "lint": "npm-run-all --parallel lint:*",
    "test": "npm run lint"
  },
  "keywords": [
    "yeoman-generator",
    "nsis",
    "nullsoft",
    "installer",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/generator-nsis"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nsis/language-data": "^0.7.4",
    "@sindresorhus/slugify": "^1.1.0",
    "globby": "^11.0.0",
    "makensis": "^0.21.1",
    "semver": "^7.1.1",
    "spdx-license-list": "^6.1.0",
    "terminal-link": "^2.1.1",
    "update-notifier": "^4.0.0",
    "yeoman-generator": "^4.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "ejs-lint": "^1.1.0",
    "eslint": "^7.6.0",
    "husky": "^4.2.5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ejs": "ejslint",
    "*.js": "eslint --cache --fix",
    "*.json": "jsonlint --quiet"
  }
}

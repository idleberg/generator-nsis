{
  "name": "generator-nsis",
  "version": "0.8.8",
  "description": "Yeoman generator for NSIS scripts",
  "main": "generators/app",
  "files": [
    "generators"
  ],
  "scripts": {
    "lint:ejs": "ejslint generators/**/*.ejs",
    "lint:js": "eslint generators/**/*.js",
    "lint:json": "jsonlint --quiet ./*.json",
    "lint": "npm-run-all --parallel lint:*",
    "publish": "np --no-yarn",
    "test": "npm run lint"
  },
  "keywords": [
    "yeoman-generator",
    "nsis",
    "nullsoft",
    "installer",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/generator-nsis"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nsis/language-data": "^0.9.0",
    "@sindresorhus/slugify": "^1.1.2",
    "globby": "^11.0.0",
    "makensis": "^1.1.1",
    "semver": "^7.3.4",
    "spdx-license-list": "^6.4.0",
    "terminal-link": "^2.1.1",
    "update-notifier": "^5.0.1",
    "yeoman-generator": "^4.12.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "ejs-lint": "^1.1.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "jsonlint": "^1.6.3",
    "lint-staged": "^11.2.6",
    "np": "^7.2.0",
    "npm-run-all": "^4.1.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ejs": "ejslint",
    "*.js": "eslint --cache --fix",
    "*.json": "jsonlint --quiet"
  }
}

{
	"name": "generator-nsis",
	"version": "0.11.0",
	"description": "Yeoman generator for NSIS scripts",
	"author": "Jan T. Sott",
	"license": "MIT",
	"keywords": [
		"yeoman-generator",
		"nsis",
		"nullsoft",
		"installer",
		"setup"
	],
	"type": "module",
	"exports": "./generators/app",
	"files": [
		"generators",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"lint:ejs": "ejslint generators/**/*.ejs",
		"lint:code": "eslint **/*.json ./generators/**/*.js --no-warn-ignored",
		"lint": "concurrently --prefix '{name}' -c 'green,blue' 'npm:lint:*'",
		"prepack": "rimraf installer.nsi license.txt",
		"prepare": "husky",
		"publish:npm": "np --any-branch",
		"test": "uvu --ignore tests/__helper.js"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/generator-nsis"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"@nsis/language-data": "^0.9.3",
		"@sindresorhus/slugify": "^2.2.1",
		"glob": "^11.0.3",
		"makensis": "3.0.0",
		"semver": "^7.7.2",
		"spdx-license-list": "^6.10.0",
		"terminal-link": "^4.0.0",
		"yeoman-generator": "^7.5.1"
	},
	"devDependencies": {
		"@lukeed/uuid": "^2.0.1",
		"@types/node": "^22.15.33",
		"concurrently": "^9.2.0",
		"ejs-lint": "^2.0.1",
		"eslint": "9.29.0",
		"eslint-plugin-jsonc": "^2.20.1",
		"eslint-plugin-unicorn": "^59.0.1",
		"husky": "^9.1.7",
		"lint-staged": "^16.1.2",
		"mem-fs": "^4.1.2",
		"np": "^10.2.0",
		"prettier": "^3.6.1",
		"rimraf": "^6.0.1",
		"tsm": "^2.3.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.0",
		"uvu": "^0.5.6",
		"yeoman-assert": "^3.1.1",
		"yeoman-environment": "^4.4.3",
		"yeoman-test": "^10.1.1"
	},
	"lint-staged": {
		"*.ejs": "ejslint",
		"*.(js|json)": [
			"eslint --cache --fix",
			"prettier --write"
		]
	},
	"packageManager": "pnpm@9.13.2+sha512.88c9c3864450350e65a33587ab801acf946d7c814ed1134da4a924f6df5a2120fd36b46aab68f7cd1d413149112d53c7db3a4136624cfd00ff1846a0c6cef48a"
}

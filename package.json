{
  "name": "generator-nsis",
  "version": "0.9.0",
  "description": "Yeoman generator for NSIS scripts",
  "author": "Jan T. Sott",
  "license": "MIT",
  "keywords": [
    "yeoman-generator",
    "nsis",
    "nullsoft",
    "installer",
    "setup"
  ],
  "main": "generators/app",
  "files": [
    "generators",
		"README.md"
  ],
  "scripts": {
    "lint:ejs": "ejslint generators/**/*.ejs",
    "lint:js": "eslint generators/**/*.mjs --ignore-path .gitignore",
    "lint:json": "eslint ./*.json --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "publish": "np --no-yarn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/generator-nsis"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@nsis/language-data": "^0.9.2",
    "@sindresorhus/slugify": "^2.2.1",
    "glob": "^10.3.10",
    "makensis": "^2.0.8",
    "semver": "^7.5.4",
    "spdx-license-list": "^6.8.0",
    "terminal-link": "^3.0.0",
    "yeoman-generator": "^7.1.1"
  },
  "devDependencies": {
    "ejs-lint": "^2.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-json": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "np": "^9.1.0",
    "npm-run-all2": "^6.1.1"
  },
  "lint-staged": {
    "*.ejs": "ejslint",
    "*.(js|json)": "eslint --cache --fix"
  }
}

{
  "name": "generator-nsis",
  "version": "0.8.9",
  "description": "Yeoman generator for NSIS scripts",
  "main": "generators/app",
  "files": [
    "generators"
  ],
  "scripts": {
    "lint:ejs": "ejslint generators/**/*.ejs",
    "lint:js": "eslint generators/**/*.js --ignore-path .gitignore",
    "lint:json": "eslint ./*.json --ignore-path .gitignore",
    "lint": "npm-run-all --parallel lint:*",
    "publish": "np --no-yarn",
    "test": "npm run lint"
  },
  "keywords": [
    "yeoman-generator",
    "nsis",
    "nullsoft",
    "installer",
    "setup"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/generator-nsis"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nsis/language-data": "^0.9.0",
    "@sindresorhus/slugify": "^1.1.2",
    "globby": "^11.1.0",
    "makensis": "^1.1.3",
    "semver": "^7.3.7",
    "spdx-license-list": "^6.6.0",
    "terminal-link": "^2.1.1",
    "update-notifier": "^5.1.0",
    "yeoman-generator": "^4.13.0"
  },
  "devDependencies": {
    "ejs-lint": "^1.2.2",
    "eslint": "^8.18.0",
    "eslint-plugin-json": "^3.1.0",
    "esprima": "^4.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "np": "^7.6.2",
    "npm-run-all": "^4.1.5"
  },
  "lint-staged": {
    "*.ejs": "ejslint",
    "*.(js|json)": "eslint --cache --fix"
  }
}
